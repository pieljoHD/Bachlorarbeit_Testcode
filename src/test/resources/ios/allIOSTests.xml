<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Smoke Tests ios" configfailurepolicy="continue" verbose="10" parallel="classes" thread-count="4">
    <listeners>
        <listener class-name="utils.ExecutionTimeListener"/>
    </listeners>
    <parameter name="platform" value="IOS"/>

    <test name="IOS Tests">
        <classes>
            <class name="tests.AddManyTodos"/>
            <class name="tests.AddAndDeleteTodos"/>
            <class name="tests.AddAndChangeTodos"/>
            <class name="tests.LoginTest"/>
            <class name="tests.AddAndChangeTodos2"/>
        </classes>
    </test>
</suite>